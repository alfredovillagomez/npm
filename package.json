{
  "name": "core-app",
  "version": "1.0.0-beta.1",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "lib",
    "README.md"
  ],
  "repository": "git@github.com:alfredovillagomez/core-app.git",
  "author": "Alfredo Villagomez <alfredo.villagomez.m@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src --ext .js,.ts,.tsx",
    "start": "bash ./tools/start.sh",
    "// future build with parcel 2.0": "$(npm bin)/parcel build 'src/index.ts' --no-cache",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build": "yarn build:esm && yarn build:cjs",
    "extract:i18n:base": "yarn lib:build && yarn format-message extract './lib/esm/**/*.js' -g underscored_crc32",
    "extract:i18n:lang": "sh -c 'yarn extract:i18n:base -l ${0} -o ./playground/i18n/${0}.json'",
    "extract:i18n": "yarn extract:i18n:lang en",
    "release": "yarn lint && bash ./tools/release.sh"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.0.0-rc.0",
    "@parcel/packager-ts": "2.0.0-rc.0",
    "@parcel/transformer-typescript-tsc": "^2.0.0-alpha.3",
    "@parcel/transformer-typescript-types": "2.0.0-rc.0",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.0",
    "gitpkg": "^1.0.0-beta.2",
    "parcel-bundler": "^1.12.5",
    "redux-logger": "^3.0.6",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "apollo-cache": "^1.3.5",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-utilities": "^1.3.4",
    "connected-react-router": "^6.9.1",
    "format-message": "^6.2.3",
    "format-message-generate-id": "^6.2.3",
    "graphql": "^15.6.0",
    "history": "^5.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.3.0",
    "typesafe-actions": "^5.1.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "gitpkg": {
    "registry": "git@github.com:alfredovillagomez/npm.git"
  }
}
